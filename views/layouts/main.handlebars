<style>
		.MycrowayInput {
			width: 100%;
			position: relative;
		}
		
		.MycrowayInput * {
			font-family: 'font-medium';
		}
		
		.MycrowayInput input {
			color: #f9f9f9;
			outline: none;
			font-family: sans-serif;
			font-size: 1.2rem;
			background: transparent;
		
			padding: 1rem 3.2rem 1rem 1.2rem;
			width: 100%;
		
			border-radius: 0.2rem;
			border: 2px solid #444;
		
			transition: border-color 0.2s;
		}
		
		.MycrowayInput input:focus {
			border-color: rgb(0,140,255);
		}
		
		.MycrowayInput input:not(:focus) {
			color: #ccc;
		}

		
		.MycrowayInput p {
			color: #444;
			font-size: 1.1rem;
			user-select: none;
		
			position: absolute;
			top: 0px;
			transform: translateY(-50%);
		
			margin-left: 0.8rem;
			padding: 0 0.4rem;
		
			background: rgb(32,33,37);
		
			transition: font-size 0.2s, color 0.2s;
		}
		
		.MycrowayInput input:focus + p,
		.MycrowayInput input:not(:placeholder-shown) + p {
			color: rgb(0,140,255);
			font-size: 1.15rem;
		}
	
	
		.MycrowayInput input:not(:focus) + p {
			font-size: 1.1rem;
			color: #444;
		}
		
				.MycrowaySelect {
			width: 100%;
			background: rgb(32,33,37);
			color: #fff;
			font-family: 'font-medium';
			height: 60px;
			border-radius: 0.2rem;
			border: 2px solid #444;
			outline: none;
			padding-left: 1rem;
		}
</style>
<style>
	.MycrowayLoading {
	width: 50px;
	height: 50px;
	border: 2px solid #008cff38;
	border-top-color: #008cff;
	border-radius: 100%;
	animation: MycrowayLoading 1s linear infinite;
}

@keyframes MycrowayLoading {
	to {
		transform: rotate(360deg);
	}
}
</style>
